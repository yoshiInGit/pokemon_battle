{
  "name": "pokemon_battle",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite -c vite.config-dev.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build -c vite.config-top.ts && vite build -c vite.config-control.ts",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint --fix",
    "format": "prettier --write ./src",
    "prepare": "husky"
  },
  "lint-staged": {
    "./src/**/*.{ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/animejs": "^3.1.12",
    "@types/howler": "^2.2.11",
    "animejs": "^3.2.2",
    "create-vite": "^5.3.0",
    "howler": "^2.2.4",
    "html2canvas": "^1.4.1",
    "pinia": "^2.1.7",
    "prettier": "^3.3.3",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.8.0",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "npm-run-all2": "^6.2.0",
    "typescript": "~5.4.0",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.3.1",
    "vite-plugin-singlefile": "^2.0.2",
    "vue-tsc": "^2.0.21"
  }
}
